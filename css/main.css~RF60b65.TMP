@charset "utf-8";
/* CSS Document */


#body{
	background:#fbfaf8;
}

.test{
	border:4px #0000CC solid;
}



/* Start Login Page */

.social_icon{
	padding-top:50px;
}

.facebook{
	background:url(../img/icon/facebook-icon2.png) no-repeat;
	height:40px;
	cursor:pointer;
}

.twitter{
	background:url(../img/icon/twitter-icon2.png) no-repeat;
	height:40px;
	cursor:pointer;
}

.google{
	background:url(../img/icon/google-icon2.png) no-repeat;
	height:40px;
	cursor:pointer;
}

.facebook:hover{
	background:url(../img/icon/facebook-icon.png) no-repeat;
	height:40px;
}

.twitter:hover{
	background:url(../img/icon/twitter-icon.png) no-repeat;
	height:40px;
}

.google:hover{
	background:url(../img/icon/google-icon.png) no-repeat;
	height:40px;
}


#login .err h6{
	color:red;
	font-size:20px;
	text-align:center;
	font-family:"Times New Roman", Times, serif;

}

#login .bottom {
	width:100%;
	margin-top:150px;
	background:#10255F;
	position:absolute;
	height:80px;
}

#login .login_form{
	width:100%;
	margin-top:9%;
	}

#login input[type="text"]
{
	height:70px;
	border-radius:8px;
	font-size:1.4em;
	color:#10255F;
	border:4px #10255f solid;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	
}

	
#login input[type="password"]
{
	height:70px;
	border-radius:8px;
	font-size:1.4em;
	color:#10255F;
	border:4px #10255f solid;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
}


#submit
{
	background-color:#10255F;
	color:#FFF;
	text-align:center;
	font-size:19px;
	padding:15px 55px 15px 55px;
	font-weight:bold;
	border-radius:10px;
	cursor:pointer;
	 -webkit-transition: 0.5s all;   
    -webkit-transition-delay: 0.5s; 
    -moz-transition: 0.5s all;   
    -moz-transition-delay: 0.5s; 
    -ms-transition: 0.5s all;   
    -ms-transition-delay: 0.5s; 
    -o-transition: 0.5s all;   
    -o-transition-delay: 0.5s; 
    transition: 0.5s all;   
    transition-delay: 0.5s; 
}

#submit:hover{
	background-color:#FFF !important;
	color:#0F5386 !important;
	padding:14px 54px 14px 54px;
 -webkit-transition-delay: 0s;
    -moz-transition-delay: 0s;
    -ms-transition-delay: 0s;
    -o-transition-delay: 0s;
    -transition-delay: 0s;
	
}


/* End Login Page */

.main_heading{
	margin-top:30px;
	font-size:18px;
	color:#10255F;
	font-family:"Comic Sans MS", cursive;
	}
	
	



#full_width{
	width:100%;
	background:#10255F;
}


.header{
	padding-top:30px;
	padding-bottom:30px;
		
}

.user_name{
	text-align:center;
}

.user_n{
	color:#fff !important;
}
.user_table{
	color:#006 !important;
}

.nav{
	padding-top:30px;
}

.nav div{
	text-align:center;
	padding-top:1.3%;
	padding-bottom:1.3%;
	font-size:18px;
	background:#4c7099;
	border-right:#fbfaf8 solid 4px;
	color:#fff;
	font-weight:bold;
	-webkit-transform: 1s;
	
}

.nav div:hover{
	background:#10255F;
	
}





/* start index page*/
.index .user_detail{
	padding-top:30px;
}


.index .heading{
	font-weight:600;
	
}

.index .title{
	font-size:16px;
	color:#035;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
}

.index .name{
	font-size:15px;
	color:#10255F;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	padding-top:20px;
}

.index .user_image{
	border:#666 6px solid;
	width:160px;
	height:210px;
	margin-top:50px;
}

/* end index page.. */


/* start signup page */

.sign_up .title{
	font-size:16px;
	color:#035;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	padding:20px 0px 10px 0px;
}

.sign_up .name{
	font-size:15px;
	color:#0F5386;
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	padding-top:8px;
}


.sign_up .block{
	padding:0px 20px 10px 20px;
	margin-top:20px;
}

.sign_up .block:hover{
	background:#F0F0F0;
}


.sign_up span{
	color:#F00;
}



input[type="text"]
{
display:inline-block;
width:210px;
height:28px;
padding:4px;
margin-bottom:9px;
font-size:13px;
line-height:18px;
color:#555555;
border:1px solid #cccccc;
-webkit-border-radius:3px;
-moz-border-radius:3px;
border-radius:3px;
background:#fff;
}

input[type="text"]:focus 
{
border-color:rgba(82, 168, 236, 0.8);
-webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
-moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
outline:0;outline:thin dotted \9;
background:#fff;
}


.sign_up form select{
padding:4px;
font-size:13px;
line-height:18px;
color:#555555;
border:1px solid #cccccc;
-webkit-border-radius:3px;
-moz-border-radius:3px;
border-radius:3px;
}

.sign_up form select:focus
{
border-color:rgba(82, 168, 236, 0.8);
-webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
-moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
outline:0;outline:thin dotted \9;
}/* CSS Document */

.sign_up textarea:focus{
	border-color:rgba(82, 168, 236, 0.8);
-webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
-moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.075),0 0 8px rgba(82, 168, 236, 0.6);
outline:0;outline:thin dotted \9;
}

input[type="date"]::-webkit-inner-spin-button{
    -webkit-appearance: none;
	text-align:right;
}

input[type="submit"]{
	margin-left:16px;
	background-color:#10255F;
	color:#FFF;
	text-align:center;
	padding:8px 16px 8px 16px;
	font-weight:bold;
	border-radius:7px;
	cursor:pointer;
	
}

input[type="submit"]:hover{
	background-color:#FFF;
	color:#0F5386;
	padding:7px 14px 7px 14px;
	border:3px #10255F solid;
}

.sign_up .exp_no
{
	margin-left:20px;
}

.sign_up .exp_yes
{
	margin-left:20px;
}

/* end signup page.. */



/* start manage_emp page */
.manage_emp{
	font-family:"Lucida Sans Unicode", "Lucida Grande", sans-serif;
	}
	
.manage_emp .table{
	padding-top:30px;
	}
	
	
.v_e_d{
	margin-left:16px;
	background-color:#10255F;
	color:#FFF;
	text-align:center;
	padding:4px 10px 4px 10px;

	border-radius:7px;
	cursor:pointer;
}


.v_e_d:hover{
	background-color:#FFF;
	color:#10255F;
	padding:2px 8px 2px 8px;
	border:2px #10255F solid;
}

.head_table{
	font-size:14px;
	font-weight:bold;
	color:#10255F;
	text-align:center;
	}
	
tr{
	padding:50px;
	}
/* end manage_emp Page */





/*start message page*/
.message{
	padding-top:60px;
	}

.icon{
	height:100px;
	width:200px;
	padding:27px;
	color:#10255F;
	text-align:center;
	font-size:19px;
	border-radius:12px;
	cursor:pointer;
	font-family:Tahoma, Geneva, sans-serif;
	border:#2970c6 solid 2px;
	
}

.icon:hover{
-webkit-box-shadow: inset 0px 0px 24px rgba(26, 109, 185, 1);
-moz-box-shadow:inset    0px 0px 24px rgba(26, 109, 185, 1);
box-shadow:inset         0px 0px 24px rgba(26, 109, 185, 1);

}

/*end message page*/





.footer{
	background:#10255F;
	height:65px;
	margin-top:50px;
	color:#fff;
}
.copyright{
	margin-top:30px;
}




